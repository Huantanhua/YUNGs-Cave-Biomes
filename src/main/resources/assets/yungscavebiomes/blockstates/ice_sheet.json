{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "yungscavebiomes:block/ice_sheet_up"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "yungscavebiomes:block/ice_sheet_down"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "yungscavebiomes:block/ice_sheet"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "yungscavebiomes:block/ice_sheet",
        "y": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "yungscavebiomes:block/ice_sheet",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "yungscavebiomes:block/ice_sheet",
        "y": 270
      }
    }
  ]
}